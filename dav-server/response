$VAR1 = bless( {
                 '_content' => '',
                 '_method' => 'GET',
                 '_protocol' => 'HTTP/0.9',
                 '_max_body_size' => undef,
                 '_headers' => bless( {}, 'HTTP::Headers' ),
                 '_uri' => bless( do{\(my $o = '/')}, 'URI::http' )
               }, 'HTTP::Request' );
